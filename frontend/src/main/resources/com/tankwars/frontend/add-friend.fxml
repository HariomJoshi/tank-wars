<AnchorPane prefHeight="339.0" prefWidth="452.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tankwars.frontend.controllers.AddFriendController">
